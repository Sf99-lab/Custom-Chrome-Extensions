{
    "manifest_version": 3,
    "name": "chrome automation",
    "version": "1.0.3",
    "description": "Open websites and click, Rotate once done.",
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "sidePanel"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Chrome automation"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "host_permissions": ["<all_urls>"]
  }