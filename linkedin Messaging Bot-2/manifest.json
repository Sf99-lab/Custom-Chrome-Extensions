{
  "manifest_version": 3,
  "name": "LinkedIn Messaging bot-2",
  "version": "1.0.2",
  "description": "Send Message by Extract LinkedIn profiles and messages from uploaded CSV file.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "LinkedIn Messenger Bot-2"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "host_permissions": [
    "https://www.linkedin.com/*"
  ]
}